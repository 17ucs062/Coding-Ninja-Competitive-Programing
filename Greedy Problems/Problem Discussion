#include<bits/stdc++.h>
using namespace std;
int main() {

    // multiset <int, greater <int> > side;
    int n,k;
    cin >> n >> k;
    int *arr = new int[n];
    for(int i = 0; i<n; i++)
    {
        // int x;
        cin >> arr[i];
        // side.insert(x);
    }
    sort(arr, arr+n);
    
    cout << arr[n-1] - k - arr[0] -k;
	// Write your code here
}
